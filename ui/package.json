  "scripts": {
    "test": "mocha -w test/helpers/browser.js test/*.spec.js",
    "clean": "rm -rf ./build",
    "lint": "eslint --fix --ext js --ext jsx src",
    "prod-build-client": "cross-env NODE_ENV=production webpack -p --config ./webpack/config.client.js --colors --display-error-details",
    "prod-build-server": "cross-env NODE_ENV=production webpack -p --config ./webpack/config.server.js --colors --display-error-details",
    "build": "npm run clean && npm run prod-build-client && npm run prod-build-server",
    "start": "node ./startServer",
    "buildandstart": "npm run build && npm start",
    "predev-start": "npm run clean",
    "dev-server": "node ./webpack/server",
    "dev-start": "sleep 30; nodemon ./startServer -e js,jsx,md --watch ./build/server",
    "dev-webpack": "cross-env NODE_ENV=development webpack --watch --config ./webpack/config.server.js --colors --display-error-details",
    "dev": "cross-env NODE_ENV=development nf start -w --procfile ./dev-tools/Procfile.dev"
  },
